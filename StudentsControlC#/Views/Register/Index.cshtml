@{
    ViewData["Title"] = "Página de Registro";
}

<h1 class="text-center">Registrar</h1>

<form>
    <div class="mb-3">
        <label for="exampleInputEmail1" class="form-label">Email</label>
        <input type="email" class="form-control">
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Senha</label>
        <input type="password" class="form-control">
    </div>
    <div class="mb-3">
        <label for="exampleInputPassword1" class="form-label">Confirme senha</label>
        <input type="password" class="form-control">
    </div>
   
    <a type="submit" class="btn btn-primary">Registrar</a>
    <a type="submit" class="btn btn-secondary" asp-area="" asp-controller="Home" asp-action="Index">Voltar</a>
</form>
